{
  "extends": "./tsconfig_extends.json",
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES2016",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6",
      "ES2015.Iterable",
      "DOM.Iterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
